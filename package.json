  "type": "module",
  "scripts": {
    "build": "spago build",
    "compile": "pnpm compile:GenerateStaticAssets && pnpm compile:superbuilder",
    "compile:GenerateStaticAssets": "spago run --main GenerateStaticAssets",
    "compile:superbuilder": "node superbuilder.js",
    "compile:Index": "pnpm compile:GenerateStaticAssets && pnpm node builders/index.js",
    "serve": "http-server ./docs"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "http-server": "^14.1.1",
    "purescript": "0.15.16-1",
    "spago": "^0.93.37"
  }
}